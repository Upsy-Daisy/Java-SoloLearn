public class Main {
   public static void main(String[] args) {
       int[][] matrix = {
               {8, 1, 6},
               {3, 5, 7},
               {4, 9, 0},
       };
       for (int[] array: matrix) {
           for (int n: array) {
               System.out.println(n);
               System.out.println("\n");
       	}
    }
  }
}